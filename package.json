{
  "name": "exstyled",
  "displayName": "ExStyled",
  "description": "Extract styled components with typedefs",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.exstyled",
    "onLanguage:typescriptreact",
    "onLanguage:javascriptreact"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "ExStyled",
      "properties": {
        "exstyled.insertImportStatement": {
          "type": "boolean",
          "default": true,
          "description": "Insert import statement of the detected library if not existent"
        }
      }
    },
    "commands": [
      {
        "command": "extension.exstyled",
        "title": "ExStyled"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.6.2",
    "@types/babel__traverse": "^7.11.1",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.56.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0"
  },
  "dependencies": {
    "@babel/generator": "^7.14.3",
    "@babel/parser": "^7.14.4",
    "@babel/traverse": "^7.14.2",
    "@babel/types": "^7.14.4"
  }
}